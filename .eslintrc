{
  "parserOptions": {
    "ecmaVersion": 6
  },

    "ecmaFeatures": {
        "blockBindings": false,
        "arrowFunctions": true,
        "defaultParams": true,
        "classes": true,
        "templateStrings": true
    },

    "rules": {
        "prefer-const": 2,
        "prefer-arrow-callback": 2,
        "no-var": 2
    }
}
